@model ThisNetWorks.OrchardCore.Seo.Meta.ViewModels.SeoMetaPartViewModel
@using ThisNetWorks.OrchardCore.Seo.Meta
@inject OrchardCore.DisplayManagement.Title.IPageTitleBuilder PageTitleBuilder
@{
    if (!String.IsNullOrEmpty(Model.PageTitle))
    {
        var seoPageTitleBuilder = PageTitleBuilder as SeoPageTitleBuilder;
        if (seoPageTitleBuilder != null)
        {
            seoPageTitleBuilder.SetTitle(Model.PageTitle);
        }
    }
}

@*<p>@Model.PageTitle</p>*@